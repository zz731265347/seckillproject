<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zz.dao.StockDao">
    <select id="getStock" parameterType="integer" resultType="Stock">
        select id, name ,count ,sale , version from stock  where id = #{id}

    </select>
    <update id="updateStockSale" parameterType="Stock">
        update stock set sale = #{sale} where id = #{id}
    </update>

</mapper>